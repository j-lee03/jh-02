{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/favicon.ico",
      "status": 404
    },
    {
      "src": "/(.*)",
      "headers": {  // <-- headers 설정이 여기로 이동
        "Content-Security-Policy": "style-src 'self' 'unsafe-inline' https://cdn.jsdelivr.net;"
      },
      "dest": "app.py"
    }
  ]
}
